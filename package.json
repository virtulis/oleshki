{
	"name": "oleshki",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"copy-deps": "cp node_modules/core-js-bundle/minified.js dist/core.js",
		"build-server": "pnpm exec tsc",
		"build-app": "exec esbuild --bundle src/web/app.tsx --inject:src/web/inject.ts --outdir=dist --sourcemap --minify",
		"build-worker": "pnpm exec esbuild --bundle src/web/worker.ts --outdir=dist --sourcemap --minify",
		"build-sass": "pnpm exec sass src/app.scss dist/app.css",
		"build-legacy": "babel --presets @babel/preset-env dist/app.js  -o dist/legacy.js --source-maps",
		"build": "(pnpm build-server & pnpm build-app & pnpm build-worker & pnpm build-sass) && wait && pnpm build-legacy",
		"watch": "(pnpm build-server --watch --preserveWatchOutput & pnpm build-app --watch & pnpm build-worker --watch & pnpm build-sass --watch) && wait"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.5",
		"@babel/core": "^7.22.1",
		"@babel/preset-env": "^7.22.4",
		"@types/geojson": "^7946.0.10",
		"@types/json-stable-stringify": "^1.0.34",
		"@types/leaflet": "^1.9.3",
		"@types/node": "^20.2.5",
		"@types/proj4": "^2.5.2",
		"@types/react": "^18.2.9",
		"@types/react-dom": "^18.2.4",
		"@typescript-eslint/eslint-plugin": "^5.59.9",
		"@typescript-eslint/parser": "^5.59.9",
		"esbuild": "^0.17.19",
		"eslint": "^8.42.0",
		"sass": "^1.63.2",
		"typescript": "^5.1.3"
	},
	"dependencies": {
		"@googleapis/sheets": "^4.0.2",
		"@sentry/browser": "^7.54.0",
		"@sentry/react": "^7.54.0",
		"@types/leaflet.locatecontrol": "^0.74.1",
		"buffer": "^6.0.3",
		"core-js-bundle": "^3.30.2",
		"csv-stringify": "^6.4.0",
		"dayjs": "^1.11.8",
		"google-auth-library": "^8.8.0",
		"json-stable-stringify": "^1.0.2",
		"leaflet": "^1.9.4",
		"leaflet.locatecontrol": "^0.79.0",
		"proj4": "^2.9.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
